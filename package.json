{
	"name": "@mikkurogue/option-ts",
	"version": "0.1.4",
	"description": "Rust-style Option<T> type for TypeScript",
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"files": [
		"dist"
	],
	"exports": {
		".": {
			"import": "./dist/index.js",
			"require": "./dist/index.js",
			"types": "./dist/index.d.ts"
		},
		"./option": {
			"import": "./dist/option/index.js",
			"require": "./dist/option/index.js",
			"types": "./dist/option/index.d.ts"
		},
		"./result": {
			"import": "./dist/result/index.js",
			"require": "./dist/result/index.js",
			"types": "./dist/result/index.d.ts"
		}
	},
	"keywords": [
		"option",
		"rust",
		"none",
		"some",
		"utility",
		"pattern-match"
	],
	"scripts": {
		"build": "tsgo",
		"test": "vitest"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/mikkurogue/option-ts"
	},
	"author": "mikkurogue",
	"license": "GPLV2",
	"devDependencies": {
		"@biomejs/biome": "^1.9.4",
		"@typescript/native-preview": "7.0.0-dev.20250710.1",
		"vitest": "^3.1.4"
	}
}
